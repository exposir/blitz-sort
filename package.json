{
  "name": "blitz-sort",
  "version": "3.0.0",
  "description": "Ultra-fast sorting library for JavaScript - up to 159x faster, averaging 19.9x speedup",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "README_ZH.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "benchmark": "npx tsx benchmark/run.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sort",
    "sorting",
    "radix-sort",
    "counting-sort",
    "fast",
    "performance",
    "algorithm",
    "uint32",
    "float64",
    "string-sort",
    "object-sort",
    "typescript"
  ],
  "author": "exposir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
